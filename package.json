{
  "name": "funkcss-tooltip",
  "version": "0.1.0",
  "description": "funkcss tooltip component",
  "main": "dist/funkcss-tooltip.min.css",
  "author": "Filipe Linhares <lip.linhares@gmail.com> (http://filipelinhares.com)",
  "homepage": "https://github.com/filipelinhares/funkcss-tooltip#readme",
  "repository": "filipelinhares/funkcss-tooltip",
  "bugs": "https://github.com/filipelinhares/funkcss-tooltip/issues",
  "license": "MIT",
  "keywords": [
    "css",
    "functional",
    "components",
    "funkcss-component"
  ],
  "scripts": {
    "test": "stylelint lib/*.css",
    "compile": "postcss -u postcss-import -u postcss-url -u postcss-for -u postcss-cssnext -u postcss-import -u postcss-nested -u postcss-reporter -o ./dist/funkcss-tooltip.css ./lib/tooltip.css",
    "minify": "postcss -u cssnano -o ./dist/funkcss-tooltip.min.css ./dist/funkcss-tooltip.css",
    "build": "npm run compile && npm run minify"
  },
  "devDependencies": {
    "cssnano": "^3.5.2",
    "cssguard": "^1.0.1",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-for": "^2.0.1",
    "postcss-import": "^8.1.0",
    "postcss-importantly": "0.0.7",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^2.0.0",
    "postcss-url": "^5.1.1",
    "stylelint": "^7.3.1"
  }
}

